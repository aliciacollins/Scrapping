<body>

    <nav class="navbar navbar-inverse navbar-fixed-top justify-content-between" role="navigation">


        <a class="navbar-brand">NFL Scrapper</a>
        <a href="#" class="navbar-brand">Home</a>
        <a id="save" class="navbar-brand">Save</a>
        <form class="form-inline  my-2 my-lg-0"></form>
        <button id="myButton" class="btn btn-lg btn-danger my-2 my-sm-0" type="button" value = "Refresh Page">Scrape New Articles</button>
        </form>

    </nav>
<div id="wrapper">
    <img src = "assets/img/arizona-cardinals.jpg"
</div>
    <br><br>
    <h1>Article Scraper</h1>
    <table>
        {{#each articles}}
        <p><b>{{title}}</b></p>
        <p>{{summary}} {{link}}
        </p>
        <hr> {{/each}}
    </table>
    <table>
        <div>Notes</div>
        {{#each notes}}
        <p>{{title}}</p>
         {{/each}}
    </table>
    

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="js/app.js"></script>

</body>